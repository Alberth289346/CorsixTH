diseases.gastric_ejections.cause = "Oorzaak: pittig Mexicaans of Indiaas eten."
diseases.gastric_ejections.cure = "Behandeling: een speciaal bindend drankje zorgt ervoor dat er niets meer uit wordt geworpen."
diseases.gastric_ejections.name = "Gastrische uitstoot"
diseases.gastric_ejections.symptoms = "Symptomen: halfverteerd voedsel wordt in willekeurige clusters uit de patiënt gestoten."

diseases.uncommon_cold.cause = "Oorzaak: deeltjes snot in de lucht."
diseases.uncommon_cold.cure = "Behandeling: een grote slok speciale hoestdrank uit de apotheek, gemaakt van speciale ingrediënten."
diseases.uncommon_cold.name = "Verguurheid"
diseases.uncommon_cold.symptoms = "Symptomen: loopneus, niezen en verkleurde longen."

diseases.corrugated_ankles.cause = "Oorzaak: het rijden over infrastructurele aanpassingen die het verkeer behoren te vertragen."
diseases.corrugated_ankles.cure = "Behandeling: het drinken van een licht bijtend kruidenmengsel zou de enkels weer recht moeten trekken."
diseases.corrugated_ankles.name = "Geribbelde enkels"
diseases.corrugated_ankles.symptoms = "Symptomen: schoeisel zit niet zo lekker."

diseases.sleeping_illness.cause = "Oorzaak: een overactieve slaapklier in het gehemelte."
diseases.sleeping_illness.cure = "Behandeling: een hoge dosis oppepmiddel wordt door een zuster toegediend."
diseases.sleeping_illness.name = "Slaapkwaal"
diseases.sleeping_illness.symptoms = "Symptomen: een onbedwingbare zin om gewoon ergens in slaap te vallen."

diseases.sweaty_palms.cause = "Oorzaak: angst voor sollicitaties."
diseases.sweaty_palms.cure = "Behandeling: een psychiater moet de patiënt overtuigen dat dit geen echte ziekte is."
diseases.sweaty_palms.name = "Zweethanden"
diseases.sweaty_palms.symptoms = "Symptomen: de hand van de patiënt voelt aan als een natte spons."

diseases.serious_radiation.cause = "Oorzaak: plutoniumisotopen verwarren met kauwgom."
diseases.serious_radiation.cure = "Behandeling: de patiënt wordt voor grondige reiniging in een ontsmettingscabine geplaatst."
diseases.serious_radiation.name = "Zware straling"
diseases.serious_radiation.symptoms = "Symptomen: patiënten met deze aandoening voelen zich heel, heel erg ziek."

diseases.gut_rot.cause = "Oorzaak: tante Triens Vrolijke Whiskey Hoestsiroop."
diseases.gut_rot.cure = "Behandeling: een zuster dient een selectie opgeloste chemicaliën toe om het buikje van binnen te bekleden."
diseases.gut_rot.name = "Buikrot"
diseases.gut_rot.symptoms = "Symptomen: geen hoest, maar ook geen maagwand."

diseases.iron_lungs.cause = "Oorzaak: smog van de binnenstad vermengd met kebabrestjes."
diseases.iron_lungs.cure = "Behandeling: twee chirurgen verwijderen de massieve longen in een operatiekamer."
diseases.iron_lungs.name = "IJzeren longen"
diseases.iron_lungs.symptoms = "Symptomen: vuurspuwen en luid onder water kunnen schreeuwen."

diseases.broken_wind.cause = "Oorzaak: een loopband gebruiken net na de maaltijd."
diseases.broken_wind.cure = "Behandeling: in de apotheek wordt er snel een zware mix van speciale waterige atomen gedronken."
diseases.broken_wind.name = "Wind tegen"
diseases.broken_wind.symptoms = "Symptomen: de patiënt verschijnt altijd chagrijnig op het werk. Dat geld ook voor mensen die dezelfde kant op fietsen."

diseases.kidney_beans.cause = "Oorzaak: kauwen op ijsklontjes uit drankjes."
diseases.kidney_beans.cure = "Behandeling: twee chirurgen verwijderen het froliet zonder gevoelige lichaamsdelen aan te raken."
diseases.kidney_beans.name = "Nierfroliet" --combinatie van Nierstenen en synoniem Nefroliet
diseases.kidney_beans.symptoms = "Symptomen: pijn en de patiënt moet heel vaak naar het toilet."

diseases.transparency.cause = "Oorzaak: het aflikken van bladfolie-lipjes van vers geopende zuivelproducten."
diseases.transparency.cure = "Behandeling: een drankje uit de apotheek, gemaakt van speciaal gekoeld en gekleurd water, verhelpt deze ziekte."
diseases.transparency.name = "Doorzichtigheid"
diseases.transparency.symptoms = "Symptomen: organen worden doorzichtig en zien er ranzig uit."

diseases.broken_heart.cause = "Oorzaak: iemand die rijker, jonger en slanker is dan de patiënt."
diseases.broken_heart.cure = "Behandeling: twee chirurgen openen de borstkas en herstellen ademloos en liefdevol het hart."
diseases.broken_heart.name = "Gebroken hart"
diseases.broken_heart.symptoms = "Symptomen: huilen en RSI door het constante verscheuren van vakantiefoto's."

diseases.slack_tongue.cause = "Oorzaak: eindeloos geklets over soaps."
diseases.slack_tongue.cure = "Behandeling: de tong wordt in de Snijmachine geplaatst en snel, efficiënt en pijnlijk verwijderd."
diseases.slack_tongue.name = "Lamme tong"
diseases.slack_tongue.symptoms = "Symptomen: tong zwelt op tot vijf maal de normale lengte."

diseases.tv_personalities.cause = "Oorzaak: dagtelevisie."
diseases.tv_personalities.cure = "Behandeling: een getrainde psychiater moet de patiënt overtuigen zijn televisie te verkopen en een radio aan te schaffen."
diseases.tv_personalities.name = "TV persoonlijkheid"
diseases.tv_personalities.symptoms = "Symptomen: de patiënt denkt dat hij of zij een kookprogramma kan presenteren."

diseases.ruptured_nodules.cause = "Oorzaak: bungeejumpen in de kou."
diseases.ruptured_nodules.cure = "Behandeling: twee gekwalificeerde chirurgen verwijderen de knobbels met vaste hand."
diseases.ruptured_nodules.name = "Gebarsten knobbels"
diseases.ruptured_nodules.symptoms = "Symptomen: niet lekker kunnen liggen."

diseases.fractured_bones.cause = "Oorzaak: van hoge dingen afvallen en op beton landen."
diseases.fractured_bones.cure = "Behandeling: er wordt gips gezet en vervolgens verwijderd met een lasergestuurde machine."
diseases.fractured_bones.name = "Botfractuur"
diseases.fractured_bones.symptoms = "Symptomen: luid gekraak en betreffend ledemaat niet meer kunnen gebruiken."

diseases.chronic_nosehair.cause = "Oorzaak: het ophalen van de neus in verachting voor mensen die slechter af zijn dan de patiënt."
diseases.chronic_nosehair.cure = "Behandeling: een smerig, haar verwijderend drankje wordt door een zuster in de apotheek klaargemaakt en wordt door de patiënt gedronken."
diseases.chronic_nosehair.name = "Chronisch neushaar"
diseases.chronic_nosehair.symptoms = "Symptomen: neushaar waarin een marter zich zou kunnen nestelen."

