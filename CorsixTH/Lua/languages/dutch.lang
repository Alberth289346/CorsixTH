trophy_room.happy_vips.trophies[1] = "Door de indruk die je hebt gemaakt op VIPs heb je een nobelprijs gewonnen. Iedereen die je ziekenhuis bezocht sprak niets dan lof."
trophy_room.happy_vips.trophies[2] = "Het Beroemde Personenbureau keert u bij dezen de Celebrity trofee uit voor de lof die u van alle VIPs heeft ontvangen. Beschouw uzelf een beetje beroemd en bijna een van ons."
trophy_room.happy_vips.trophies[3] = "Gefeliciteerd met het winnen van de VIP TRIP. U maakt de levens van hardwerkende beroemdheden een stuk mooier, door het ze allemaal naar hun zin te maken. Fantastisch!"

trophy_room.rats_killed.trophies[1] = "Je hebt de Anti-Ongedierte trofee gewonnen voor het schieten van %d ratten in je ziekenhuis in het afgelopen jaar."
trophy_room.rats_killed.trophies[2] = "Deze trofee wordt aangeboden door de Federatie Tegen Ratten en Muizen om uw uitzonderlijke schietwerk op %d ratten te eren."
trophy_room.rats_killed.trophies[3] = "Deze Rattenknaller trofee wordt u aangeboden voor het afschieten van %d ratten in het afgelopen jaar."

trophy_room.rats_accuracy.trophies[1] = "U krijgt de Syndicate Wars Precisie trofee omdat %d%% van de schoten die u op ratten afvuurde doel troffen."
trophy_room.rats_accuracy.trophies[2] = "Deze trofee eert uw fantastische precisie in het afschieten van  %d%% van de ratten waar u in het afgelopen jaar op gemikt heeft."
trophy_room.rats_accuracy.trophies[3] = "Om te vieren dat u afgelopen jaar %d%% van alle ratten in uw ziekenhuis heeft uitgeroeid, belonen wij u met de Dungeon Keeper Ongediertevrij trofee. ProficiRAT"

trophy_room.all_cured.awards[1] = "Gefeliciteerd met het winnen van de Marie Curie Award voor het behandelen van alle patiënten in uw ziekenhuis in het afgelopen jaar."
trophy_room.all_cured.trophies[1] = "De Internationale Geneesstichting wil u eren met de Gezondheid voor Iedereen trofee voor het genezen van iedereen die in het afgelopen jaar uw ziekenhuis binnen is komen lopen."
trophy_room.all_cured.trophies[2] = "U krijgt de Geen Zieke Rochel trofee voor het genezen van alle patiënten die in het afgelopen jaar uw ziekenhuis hebben bezocht."

trophy_room.many_cured.awards[1] = "Gefeliciteerd met het winnen van de Marie Curie Award voor het behandelen van bijna alle patiënten in uw ziekenhuis in het afgelopen jaar."
trophy_room.many_cured.awards[2] = "U ontvangt deze trofee omdat u meer patiënten heeft genezen dan alle andere ziekenhuizen. Een opvallende prestatie."
trophy_room.many_cured.trophies[1] = "De Internationale Geneesstichting wil u eren met de Gezondheid voor Alles trofee voor het genezen van massa's mensen die in het afgelopen jaar uw ziekenhuis binnen zijn komen lopen."
trophy_room.many_cured.trophies[2] = "U krijgt de onderscheiding Het Kwieke Hoentje voor het genezen van een groot aantal patiënten dat in het afgelopen jaar uw ziekenhuis heeft bezocht."
trophy_room.many_cured.trophies[3] = "Hierbij ontvangt u de Gezonde Gorgel, een prijs voor het genezen van een enorm percentage van uw bezoekers in het afgelopen jaar."
trophy_room.many_cured.penalty[1] = "Het lukt uw ziekenhuis niet de patiënten effectief te genezen. Concentreer u op goede behandelmethoden, zodat behandelingen effectiever worden."
trophy_room.many_cured.penalty[2] = "Uw ziekenhuis is het minst effectief als het gaat om patiënten genezen. U heeft het ministerie teleur gesteld. Moeten we nog meer zeggen?"

trophy_room.many_cured.regional[1] = "Vanwege het feit dat u meer zieken heeft genezen dan alle andere ziekenhuizen bij elkaar opgeteld, ontvangt u hiervoor de hoogst haalbare onderscheiding. Gefeliciteerd!"

trophy_room.healthy_plants.awards[1] = "Gefeliciteerd met het winnen van de Oei Ik Groei award voor de fantastische staat van de planten in uw ziekenhuis."
trophy_room.healthy_plants.trophies[1] = "De Vrienden van de Potplanten willen u de Groene Gezondheid trofee uitreiken voor de blakende gezondheid waarin uw planten zich de afgelopen twaalf maanden hebben verkeerd"
trophy_room.healthy_plants.trophies[2] = "De Frisse Narcissen willen u de Groene Vingers trofee uitreiken voor de uitstekende gezondheid van uw planten in het afgelopen jaar."

trophy_room.no_deaths.awards[1] = "U heeft de Nimmer Vergaan award gewonnen voor het in leven houden van al uw patiënten in het afgelopen jaar."
trophy_room.no_deaths.awards[2] = "Uw talent heeft gezorgd voor een minimaal aantal sterfgevallen in uw ziekenhuis. U kunt apetrots zijn op dit resultaat."
trophy_room.no_deaths.trophies[1] = "De 'Het Leven Gaat Door'-corporatie presenteert u deze trofee omdat er in het afgelopen jaar geen mensen zijn overleden."
trophy_room.no_deaths.trophies[2] = "U heeft de Overlijden Vermijden trofee uitgereikt gekregen voor het in leven houden van elke bezoeker van uw fantastische ziekenhuis in het afgelopen jaar. Bravo."
trophy_room.no_deaths.trophies[3] = "Hierbij ontvangt u de 'Ik Leef Nog' prijs omdat dit jaar geen van uw patiënten is overleden in uw mooie ziekenhuis. Heel goed."
trophy_room.no_deaths.penalty[1] = "Het aantal overledenen in uw ziekenhuis was het afgelopen jaar behoorlijk hoog. Let goed op en zorg dat in de toekomst meer mensen de behandeling overleven."
trophy_room.no_deaths.penalty[2] = "Uw ziekenhuis is een gevaar voor de gezondheid van de patiënt. Er wordt verwacht dat u mensen geneest, niet laat doodgaan!"
trophy_room.no_deaths.regional[1] = "Het dodental in uw ziekenhuis was het laatste jaar lager dan dat van ieder ander ziekenhuis. Daarvoor bieden wij u deze trofee aan."

trophy_room.sold_drinks.trophies[1] = "De Wereld Tandartsen Vereniging is trots om u deze trofee en plak te presenteren voor het verkopen van massa's frisdrank in uw ziekenhuis."
trophy_room.sold_drinks.trophies[2] = "Uw ziekenhuis heeft de Frisse Zaak trofee gewonnen voor de grote hoeveelheid drankjes die uw gasten in het afgelopen jaar hebben gekocht."
trophy_room.sold_drinks.trophies[3] = "Uit naam van Goedgevuld Inc. presenteren wij u vol trots deze met chocola overgoten trofee ter ere van uw uitzonderlijk hoge aantal verkochte blikjes frisdrank."

trophy_room.high_rep.awards[1] = "Hierbij eren wij uw hoge standaarden en een reputatie die dit jaar gewoon niet beter kan zijn. Goed werk."
trophy_room.high_rep.awards[2] = "Gefeliciteerd met de Zuijver Op Den Graet trofee voor het ziekenhuis met de hoogste reputatie van het land. En het is welverdiend ook."

trophy_room.high_rep.penalty[1] = "Deze maatregel wordt genomen vanwege de zeer slechte reputatie over het afgelopen jaar. We gaan ervan uit dat u verbetering laat zien in de toekomst."
trophy_room.high_rep.penalty[2] = "De reputatie van uw ziekenhuis is het laagst in de omgeving. U bent een schande. Laat verbetering zien, of zoek een andere baan!"

trophy_room.high_rep.regional[1] = "Wij bieden u de Bullfrog Award aan voor de hoogste reputatie van alle ziekenhuizen in het afgelopen jaar. Geniet ervan - u heeft het verdiend."
trophy_room.high_rep.regional[2] = "Dit jaar behaalde uw ziekenhuis een reputatie die hoger is dan alle andere ziekenhuizen bij elkaar. Een enorme prestatie."

trophy_room.consistant_rep.trophies[1] = "Hierbij ontvangt u de hoogste onderscheiding van het ministerie voor de meest onberispelijke standaarden en de hoogst mogelijke reputatie dit jaar. Heel goed."
trophy_room.consistant_rep.trophies[2] = "Proficiat met het behalen van de Brandschoon Trofee voor het ziekenhuis met de beste reputatie in het afgelopen jaar. Welverdiend."

trophy_room.happy_patients.awards[1] = "U mag trots zijn op het feit dat bezoekers van uw ziekenhuis in het afgelopen jaar bijzonder tevreden waren."
trophy_room.happy_patients.awards[2] = "Bezoekers van uw ziekenhuis waren gemiddeld meer tevreden tijdens hun behandeling, dan bezoekers van andere ziekenhuizen in het spel."
trophy_room.happy_patients.penalty[1] = "Bezoekers van uw ziekenhuis vinden het verschrikkelijk. U zult veel beter moeten presteren om waardering van het ministerie te krijgen."
trophy_room.happy_patients.penalty[2] = "Mensen die in uw ziekenhuis in behandeling zijn, zijn zeer ontevreden over de staat van het ziekenhuis. U moet meer aandacht schenken aan het wel en wee van de patiënten."
trophy_room.happy_staff.awards[1] = "Deze oorkonde ontvangt u van uw eigen personeel. Er is natuurlijk nog ruimte voor verbetering, maar over het algemeen wordt u als werkgever zeer gewaardeerd."
trophy_room.happy_staff.awards[2] = "Uw personeel is zo tevreden en gemotiveerd, niets kan hun glimlach nog verpesten. U bent een geweldige manager!"
trophy_room.happy_staff.trophies[1] = "U krijgt de Smiley trofee voor het zo gelukkig mogelijk houden van uw hardwerkende personeel."
trophy_room.happy_staff.trophies[2] = "Het Adami Instituut van Tevredenheid keert u hierbij deze trofee uit omdat uw personeel geen moment blijk heeft gegeven van ontevredenheid."
trophy_room.happy_staff.trophies[3] = "Deze trofee, de Straalmeer Cup, is hierbij de uwe voor uw inzet voor het welzijn en de tevredenheid van uw personeel. Een glimlach van iedereen!"
trophy_room.happy_staff.penalty[1] = "Uw personeel laat officieel weten dat ze ontevreden zijn. Gemotiveerd personeel is waardevol. Werk aan hun tevredenheid voor u ze helemaal zult verliezen."

trophy_room.happy_staff.regional_good[1] = "Uw personeel is meer tevreden dan dat van andere ziekenhuizen. Tevreden personeel betekent meer inkomsten en minder sterfgevallen. Dit wordt door het ministerie zeer gewaardeerd."
trophy_room.happy_staff.regional_bad[1] = "Uw personeel heeft zich het afgelopen jaar ontevreden getoond. Heeft u dat niet gemerkt? Op dit moment heeft ieder ander ziekenhuis meer tevreden personeel dan uw ziekenhuis."

--attention: this string says AWARD, not AWARDS
trophy_room.wait_times.award[1] = "Gefeliciteerd. Uw ziekenhuis heeft altijd zeer korte wachttijden. Dit is een belangrijke onderscheiding."
trophy_room.wait_times.penalty[1] = "Patiënten in uw ziekenhuis moeten veel te lang wachten. Er zijn altijd enorme wachtrijen. U kunt uw patiënten veel efficiënter helpen, maar daar moet u wel wat voor doen."

trophy_room.hosp_value.awards[1] = "Het Ministerie van Volksgezondheid wil dit moment graag gebruiken om u te feliciteren met de indrukwekkende waarde die uw ziekenhuis heeft behaald."
trophy_room.hosp_value.penalty[1] = "Uw ziekenhuis heeft gefaald een behoorlijke waarde te behalen. U heeft slechte financiële keuzes gemaakt. Houd in uw achterhoofd dat een goed ziekenhuis niet goedkoop is."
trophy_room.hosp_value.regional[1] = "U bent een financieel wonder! Uw ziekenhuis is meer waard dan alle andere ziekenhuizen in de omgeving bij elkaar opgeteld."
trophy_room.best_value_hosp.trophies[1] = "Proficiat met het behalen van de Brandschoon trofee voor het ziekenhuis met de beste reputatie in het afgelopen jaar. Welverdiend."
trophy_room.best_value_hosp.penalty[1] = "Elke ziekenhuis in de omgeving is meer waard dan die van u. Doe iets aan deze beschamende gang van zaken. Zorg voor mooie en dure apparatuur!"
trophy_room.best_value_hosp.regional[1] = "Gefeliciteerd met het meest waardevolle ziekenhuis in het spel, goed werk. Probeer dat zo te houden."

trophy_room.curesvdeaths.awards[1] = "Hartelijk gefeliciteerd met het behalen van een indrukwekkende genezen-of-overleden rato in uw ziekenhuis in het afgelopen jaar."
trophy_room.curesvdeaths.penalty[1] = "Uw genezen-of-overleden rato is erg slecht. U moet ervoor zorgen u veel meer mensen geneest dan laat sterven. Laat het er niet bij zitten."

--attention: this string says AWARD, not AWARDS
trophy_room.cleanliness.award[1] = "Inspecteurs vinden uw ziekenhuis heel schoon. Schone ziekenhuizen zijn veilige ziekenhuizen. Houden zo."
trophy_room.cleanliness.regional_good[1] = "Uw ziekenhuis wordt genoemd als een van de minst schone in de omgeving. Een smerig ziekenhuis stinkt en is gevaarlijk. U moet meer aandacht hebben voor troep."
trophy_room.cleanliness.regional_bad[1] = "Uw ziekenhuis is het smerigst van de regio. Alle andere managers is het gelukt hun gangen schoner te houden. U bent een schande voor de medische wereld."

trophy_room.gen_repairs.awards[1] = "U ontvangt deze prijs vanwege de oplettendheid van uw conciërges die alle apparatuur heel goed hebben onderhouden. Heel goed. U kunt gerust op vakantie."
trophy_room.gen_repairs.awards[2] = "Uw conciërges hebben beter gepresteerd dan die van andere ziekenhuizen. Dit is een enorme prestatie. Ere wie ere toekomt."
trophy_room.gen_repairs.awards[3] = "Uw apparatuur is fantastisch onderhouden. De motivatie van uw conciërges in uitmuntend. Deze prijs heeft u meer dan verdiend. Geweldig!."
trophy_room.gen_repairs.penalty[1] = "Uw conciërges hebben uw apparatuur niet goed onderhouden. U moet hen beter in de gaten houden, of meer conciërges aannemen als het teveel werk is."
trophy_room.gen_repairs.penalty[2] = "Het onderhoud is een drama. Uw personeel moet de apparatuur beter in de gaten houden en zorgvuldiger onderhouden."

--attention: this string says AWARD, not AWARDS
trophy_room.emergencies.award[1] = "Gefeliciteerd: vanwege uw efficiënte en effectieve manier van afhandelen van noodgevallen, ontvangt u deze speciale prijs. Goed werk."
trophy_room.emergencies.award[2] = "Uw werkwijze bij noodgevallen is een unicum. Deze prijs ontvangt u, omdat u absoluut het beste omgaat met grote drommen zieken en zwakken."
trophy_room.emergencies.penalty[1] = "U heeft noodgevallen niet goed afgehandeld. Spoed eisende patiënten moeten direct en goed worden geholpen, dat is u niet gelukt."
trophy_room.emergencies.regional_good[1] = "Het ministerie heeft het afgelopen jaar gezien dat u noodgevallen beter aankunt dan ieder ander ziekenhuis. Daarvoor ontvangt u deze prijs."
trophy_room.emergencies.regional_bad[1] = "Uw ziekenhuis doet het het slechtst van de regio als het gaat om noodgevallen. Het is uw eigen fout dat u onderaan de lokale noodgevallen ranglijst bungelt."

trophy_room.pop_percentage.awards[1] = "Heeft u gezien dat een groot deel van de lokale bevolking voor uw ziekenhuis heeft gekozen? Goed gedaan."
trophy_room.pop_percentage.awards[2] = "Gefeliciteerd. Vergeleken met andere medische instellingen heeft uw ziekenhuis het grootste aandeel in de bezoekers uit de regio."
trophy_room.pop_percentage.awards[3] = "Briljant! U heeft meer lokale bewoners naar uw ziekenhuis weten te lokken dan alle andere ziekenhuizen bij elkaar opgeteld."
trophy_room.pop_percentage.penalty[1] = "Heel weinig mensen hebben dit jaar uw ziekenhuis bezocht. Om geld te verdienen heeft uw ziekenhuis voldoende betalende zieken nodig!"
trophy_room.pop_percentage.penalty[2] = "Elk ziekenhuis in de omgeving heeft een groter aandeel lokale bewoners mogen begroeten dan u. Schaamt u zich niet een beetje?"

trophy_room.research.awards[1] = "Uw onderzoekers hebben er het afgelopen jaar voor gezorgd dat uw ziekenhuis volledig up-to-date is met de nieuwste ontwikkelingen. Uw personeel heeft deze onderscheiding verdiend. Heel goed."
trophy_room.research.awards[2] = "Het afgelopen jaar heeft u meer medicijnen en apparatuur ontwikkeld dan was verwacht. Wij bieden u deze prijs aan namens het volledige ministerie."
trophy_room.research.penalty[1] = "U heeft een minimale hoeveelheid behandelmethoden, medicijnen en apparatuur ontwikkeld. Dit is helemaal niet goed, technologische vooruitgang is essentieel."
trophy_room.research.regional_good[1] = "Uw onderzoekers hebben er het afgelopen jaar voor gezorgd dat uw ziekenhuis volledig up-to-date is met de nieuwste ontwikkelingen. Uw personeel heeft deze onderscheiding verdiend. Heel goed."
trophy_room.research.regional_bad[1] = "Ieder ander ziekenhuis in de regio heeft meer en beter onderzoek gedaan dan u. Omdat onderzoek cruciaal is voor een ziekenhuis, noemt het ministerie dit onacceptabel."

